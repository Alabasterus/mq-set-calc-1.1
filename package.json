{
  "devDependencies": {
    "6to5": "^2.9.4",
    "bluebird": "^2.6.2",
    "globby": "^1.1.0",
    "jspm": "^0.10.4",
    "lodash": "^2.4.1"
  },
  "jspm": {
    "dependencies": {
      "bootstrap": "^3.3.1",
      "jquery": "^2.1.3",
      "jsx": "^0.1.1",
      "lodash": "^2.4.1",
      "react": "^0.12.2"
    },
    "buildConfig": {
      "transpileES6": true,
      "minify": true
    }
  },
  "repository": "https://github.com/Alabasterus/mq-set-calc-1.1",
  "scripts": {
    "install": "jspm install",
    "bundle": "jspm bundle app/main --minify",
    "manifest": "6to5-node --experimental manifest.js > manifest.appcache",
    "clean": "rm -f build.js* manifest.appcache",
    "rebuild": "npm run clean && npm run bundle && npm run manifest"
  }
}
